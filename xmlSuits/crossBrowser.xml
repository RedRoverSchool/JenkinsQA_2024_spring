<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="CrossBrowser" parallel="methods" thread-count="1" data-provider-thread-count="2">

    <test name="ChromeTest" verbose="10">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="school.redrover.FreestyleProjectTest"/>
        </classes>
    </test>

    <test name="ChromiumTest" verbose="10">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="school.redrover.FolderTest"/>
        </classes>
    </test>
</suite>